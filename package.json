{
  "name": "thebrainyinsights-replatform",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:admin": "pnpm --filter @tbi/admin dev",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:admin\"",
    "build": "next build",
    "build:admin": "pnpm --filter @tbi/admin build",
    "build:all": "pnpm build && pnpm build:admin",
    "start": "next start",
    "start:admin": "pnpm --filter @tbi/admin start",
    "lint": "next lint --fix",
    "lint:admin": "pnpm --filter @tbi/admin lint",
    "lint:all": "pnpm lint && pnpm lint:admin",
    "typecheck": "tsc --noEmit",
    "db:generate": "cd packages/database && pnpm db:generate",
    "db:push": "npx pnpm exec dotenv -e .env.local -- cd packages/database && npx pnpm db:push",
    "clean:all": "rd /s /q node_modules packages\\\\database\\\\node_modules apps\\\\admin\\\\node_modules .next 2>nul & del pnpm-lock.yaml 2>nul",
    "fresh-install": "pnpm run clean:all && pnpm install --no-frozen-lockfile"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "5.7.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tbi/database": "workspace:*",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cuid": "^3.0.0",
    "lru-cache": "^10.0.0",
    "lucide-react": "^0.542.0",
    "next": "15.0.3",
    "next-auth": "^4.0.0",
    "next-intl": "^4.3.5",
    "openai": "4.20.0",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "10.4.16",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.1",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.0",
    "postcss": "8.4.32",
    "prisma": "5.7.1",
    "tailwindcss": "3.4.0",
    "tsx": "4.6.0",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "next": "15.0.3",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@next/env": "15.0.3",
      "@next/swc-darwin-arm64": "15.0.3",
      "@next/swc-linux-x64-gnu": "15.0.3",
      "@next/swc-win32-x64-msvc": "15.0.3"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  },
  "resolutions": {
    "next": "15.0.3"
  }
}
