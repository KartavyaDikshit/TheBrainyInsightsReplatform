{
  "compilerOptions": {
    "target": "es2016",
    "module": "nodenext", // Changed from "esnext" to "nodenext"
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["scripts/**/*.ts"]
}